{
    "name": "clementdecou/simply-framework",
    "type": "wordpress-muplugin",
    "license": "MIT",
    "require": {
        "php": ">= 8.0",
        "symfony/dependency-injection": "^6.0",
        "symfony/config": "^6.0",
        "symfony/finder": "^6.0",
        "symfony/yaml": "^6.0",
        "symfony/proxy-manager-bridge": "^6.0",
        "twig/twig": "^3.3"
    },
    "require-dev": {
        "brain/monkey": "~2.0.0",
        "phpunit/phpunit": "~9.5"
    },
    "autoload": {
        "psr-4": {
            "Simply\\Core\\": "src",
            "Simply\\Tests\\": "tests"
        }
    },
    "exclude-from-classmap": [
        "/Tests/"
    ]
}
